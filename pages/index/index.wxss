/**index.wxss**/
page{
  width: 100%;
  height: 100%;
}
.container{
  position: relative;
  height: 100%;
  width: 100%;
}
.canvas-area{
  width:100%;
  height: 100%;
  background-color: #f8f8f8;
}
.myCanvas{
  width: 100%;
  height: 100%;
}
.canvas-tools{
  position: fixed;
  left: 0;
  bottom: 20rpx;
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.box{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: rebeccapurple;
}
.box1{
  background-color: #99cccc;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-position: center;
}
